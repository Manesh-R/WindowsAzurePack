<?xml version="1.0" encoding="utf-8"?>
<uiManifest>
  <!--Storage Sample-->
  <extension name="StorageSampleTenantExtension"
             baseUri="~/Content/StorageSampleTenant">
    <scripts>
      <script src="~/Scripts/ViewModel/HW.ViewModels.QuickCreate.js" />
      <script src="~/Scripts/StorageSampleTenant.Controller.js" />
      <script src="~/Scripts/StorageSampleTenant.ContainersTab.js" />
      <script src="~/Scripts/StorageSampleTenant.SettingsTab.js" />
      <script src="~/StorageSampleTenantExtension.js" />

      <!--Self registration-->
      <script src="~/extensions.data.js" />
    </scripts>
    <stylesheets>
      <stylesheet src="~/Styles/StorageSampleTenant.css"/>
    </stylesheets>
    <templates>
      <template name="customCreatePreview"
                src="~/Templates/Dialogs/CustomCreatePreview.html" />
      <template name="CreateContainer"
                src="~/Templates/Dialogs/CreateContainer.html" />
      <template name="ContainersTab"
                src="~/Templates/Tabs/ContainersTab.html" />
      <template name="ContainersTabEmpty"
                src="~/Templates/Tabs/ContainersTabEmpty.html" />
      <template name="settingsTab"
                src="~/Templates/Tabs/SettingsTab.html" />
    <template name="quickCreateContainer"
                src="~/Templates/Menu/ContainerQuickCreate.html" />
    </templates>
  </extension>
</uiManifest>
